              reading_time: "12 min",
            },
          ];
        } else if (
          job?.name?.toLowerCase().includes("ecommerce") ||
          job?.name?.toLowerCase().includes("product")
        ) {
          demoData = [
            {
              id: 1,
              product_name: "Wireless Bluetooth Headphones",
              price: "$129.99",
              url: `${job?.url || "https://example.com"}/product/1`,
              description:
